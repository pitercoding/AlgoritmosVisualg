algoritmo "Contar 0 até onde quiser"
var
   valor, contador: Inteiro
inicio
    Escreva ("Digite um número: ")
    Leia(valor)
    contador <- 0
    Enquanto  (contador <= valor) faca
              EscrevaL (contador)
              contador <- contador +1
    FimEnquanto
    EscrevaL ("Terminei de contar")
fimalgoritmo
